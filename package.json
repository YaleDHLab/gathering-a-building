{
  "name": "gathering-a-building",
  "version": "1.0.0",
  "description": "A research site on Yale's architectural history",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server",
    "build": "webpack",
    "deploy": "npm run build && npm run deploy-unzipped && npm run deploy-gzipped",
    "upload-geotiffs": "aws s3 cp geotiffs s3://gathering-a-building --recursive --acl public-read --exclude .DS_Store",
    "deploy-unzipped": "aws s3 cp ../gathering-a-building/build s3://gathering-a-building-deploy --recursive --acl public-read --exclude bundle.js",
    "deploy-gzipped": "aws s3 cp ../gathering-a-building/build/bundle.js s3://gathering-a-building-deploy --content-encoding 'gzip' --acl public-read",
    "authorize": "node app/js/lib/request-autodesk-token.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YaleDHLab/gathering-a-building.git"
  },
  "keywords": [
    "architecture",
    "digital-humanities",
    "yale"
  ],
  "author": "DHLab@Yale",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/YaleDHLab/gathering-a-building/issues"
  },
  "homepage": "https://github.com/YaleDHLab/gathering-a-building#readme",
  "devDependencies": {
    "angular": "^1.5.8",
    "async": "^2.0.1",
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "compression-webpack-plugin": "^0.3.1",
    "css-loader": "^0.25.0",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "less": "^2.7.1",
    "style-loader": "^0.13.1",
    "superagent": "^2.2.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1",
    "webpack-merge": "^0.14.1"
  },
  "dependencies": {
    "angular-route": "^1.5.8",
    "angular-sanitize": "^1.5.8",
    "angularjs-slider": "^5.5.0",
    "d3": "^4.2.2",
    "font-awesome": "^4.6.3",
    "font-awesome-webpack": "0.0.4",
    "jquery": "^2.2.2",
    "leaflet": "^0.7.7"
  }
}
